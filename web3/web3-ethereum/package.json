{
  "name": "@exodus/web3-ethereum",
  "description": "Apps connectivity library for EVM-compatible chains.",
  "version": "13.5.0",
  "type": "module",
  "sideEffects": false,
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "exports": {
    ".": "./lib/index.js",
    "./package.json": "./package.json",
    "./provider": "./lib/provider/index.js",
    "./provider-proxy": "./lib/provider-proxy/index.js",
    "./rpc": "./lib/rpc-handlers/index.js",
    "./wallet-standard": "./lib/wallet-standard/index.js",
    "./eip6963": "./lib/eip6963/index.js"
  },
  "react-native": "./lib/index.js",
  "files": [
    "lib/**/*.d.ts",
    "lib/**/*.mjs",
    "lib/**/*.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ExodusMovement/assets.git"
  },
  "license": "MIT",
  "scripts": {
    "clean": "run -T rimraf lib",
    "build": "yarn clean && run -T tsc && yarn build:schemas",
    "build:schemas": "node ../scripts/build-schemas/index.mjs",
    "lint": "run -T eslintc .",
    "lint:fix": "yarn lint --fix",
    "prepublishOnly": "yarn run -T build --scope @exodus/web3-ethereum",
    "test": "run -T exodus-test --jest --esbuild"
  },
  "dependencies": {
    "@exodus/ethereum-api": "^8.31.0",
    "@exodus/web3-constants": "^1.30.0",
    "@exodus/web3-ethereum-utils": "^4.4.0",
    "@exodus/web3-provider": "^3.15.0",
    "@exodus/web3-rpc-handlers": "^2.25.0",
    "@exodus/web3-utils": "^1.48.2",
    "bn.js": "^5.2.0",
    "eth-rpc-errors": "^4.0.3"
  },
  "devDependencies": {
    "@exodus/assets": "workspace:^",
    "@exodus/ethereum-meta": "workspace:^",
    "@exodus/json-rpc": "^1.11.0",
    "@exodus/schemasafe": "^1.0.0",
    "@exodus/web3-management": "workspace:*",
    "@exodus/web3-types": "workspace:*",
    "@exodus/web3-validation": "^1.38.0",
    "@wallet-standard/base": "^1.0.1"
  },
  "author": "Exodus Movement, Inc.",
  "bugs": {
    "url": "https://github.com/ExodusMovement/assets/issues?q=is%3Aissue+is%3Aopen+label%3Aweb3-ethereum"
  },
  "homepage": "https://github.com/ExodusMovement/assets/tree/master/web3/web3-ethereum"
}
